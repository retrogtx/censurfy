{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./app/*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/constants/*": [
        "./constants/*"
      ],
      "@/store/*": [
        "./store/*"
      ],
      "@/utils/*": [
        "./utils/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/schemas/*": [
        "./schemas/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
